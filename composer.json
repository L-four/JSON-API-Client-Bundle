{
  "name": "enm/json-api-client-bundle",
  "description": "Symfony integration for enm/json-api-client",
  "type": "symfony-bundle",
  "require": {
    "php": ">=7.2",
    "enm/json-api-client": "^3.0",
    "symfony/dependency-injection": "^4.0",
    "symfony/http-kernel": "^3.0|^4.0",
    "symfony/config": "^3.0|^4.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.0",
    "guzzlehttp/guzzle": "^6.3"
  },
  "suggest": {
    "kriswallsmith/buzz": "To use buzz as http client",
    "e-moe/guzzle6-bundle": "To use guzzle as http client",
    "eightpoints/guzzle-bundle": "An other way to use guzzle as http client"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Philipp Marien",
      "email": "marien@eosnewmedia.de"
    }
  ],
  "autoload": {
    "psr-4": {
      "Enm\\Bundle\\JsonApi\\Client\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Enm\\Bundle\\JsonApi\\Client\\Tests\\": "tests"
    }
  },
  "minimum-stability": "stable"
}

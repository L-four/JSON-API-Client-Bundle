{
  "name": "enm/json-api-client-bundle",
  "description": "Symfony integration for enm/json-api-client",
  "type": "symfony-bundle",
  "require": {
    "enm/json-api-client": "^1.0",
    "symfony/dependency-injection": "^2.7|^3.0|^4.0",
    "symfony/http-kernel": "^2.7|^3.0|^4.0",
    "symfony/config": "^2.7|^3.0|^4.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.3",
    "guzzlehttp/guzzle": "^6.3"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Philipp Marien",
      "email": "marien@eosnewmedia.de"
    }
  ],
  "autoload": {
    "psr-4": {
      "Enm\\Bundle\\JsonApi\\Client\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Enm\\Bundle\\JsonApi\\Client\\Tests\\": "tests"
    }
  },
  "config": {
    "platform": {
      "php": "7.0"
    }
  },
  "minimum-stability": "stable"
}
